<section>
    <div class="section-section">

        <hr class="section-hr">

        <div class="section-infor">
            <div class="infor-member">
                <div class="member-memberleft">
                    <div class="memberleft-nickname">
                        <p class="nickname-nickname"><%- member[0].hname; %></p>
                        <p class="nickname-end">님</p>
                    </div>

                    <div class="memberleft-classlevel">
                        <p class="classlevel-label">LV</p>
                        <p class="classlevel-name" style=<%- classlevelcss %>><%- member[0].classlevelhname; %></p>
                    </div>
                </div>

                <div class="member-memberright">
                    <div class="memberright-cashpointown">
                        <p class="cashpointown-text">보유</p>
                        <p class="cashpointown-ea"><%- member[0].membercp.toLocaleString(); %></p>
                        <p class="cashpointown-unit">CP</p>
                    </div>

                    <div class="memberright-cashpointexchange">
                        <p class="cashpointexchange-text">미전환</p>
                        <p class="cashpointexchange-ea"><%- ((member[0].memberpointexp * 1000) + (member[0].memberpointmctk * 55) + (member[0].memberpointnton * 3300) + (member[0].memberpointtrvt * 11)).toLocaleString()  %></p>
                        <p class="cashpointexchange-unit">CP</p>
                    </div>
                </div>
            </div>
        </div>

        <hr class="section-hr">

        <div class="section-points">
            <div class="points-point point-exp">
                <div class="point-infor">
                    <div class="infor-pointname">
                        <img class="pointname-image" src="/icon/point_symbol_mctk.svg" alt="">
                        <p class="pointname-name">EXP</p>
                    </div>

                    <div class="infor-rate">
                        <p class="rate-ea1">1,000.</p>
                        <p class="rate-ea2">0000</p>
                        <p class="rate-unit">CP</p>
                    </div>
                </div>

                <div class="point-own">
                    <div class="own-point">
                        <p class="point-ea color-exp"><%- member[0].memberpointexp.toLocaleString(); %></p>
                        <p class="point-unit color-exp">EXP</p>
                    </div>

                    <div class="own-exchange">
                        <p class="exchange-ea"><%- (member[0].memberpointexp * 1000).toLocaleString() %></p>
                        <p class="exchange-unit">CP</p>
                    </div>
                </div>
            </div>

            <div class="points-point point-mctk">
                <div class="point-infor">
                    <div class="infor-pointname">
                        <img class="pointname-image" src="/icon/point_symbol_mctk.svg" alt="">
                        <p class="pointname-name">Mine Token</p>
                    </div>

                    <div class="infor-rate">
                        <p class="rate-ea1">55.</p>
                        <p class="rate-ea2">0000</p>
                        <p class="rate-unit">CP</p>
                    </div>
                </div>

                <div class="point-own">
                    <div class="own-point">
                        <p class="point-ea color-mctk"><%- member[0].memberpointmctk.toLocaleString(); %></p>
                        <p class="point-unit color-mctk">MCTK</p>
                    </div>

                    <div class="own-exchange">
                        <p class="exchange-ea"><%- (member[0].memberpointmctk * 55).toLocaleString() %></p>
                        <p class="exchange-unit">CP</p>
                    </div>
                </div>
            </div>

            <div class="points-point point-nton">
                <div class="point-infor">
                    <div class="infor-pointname">
                        <img class="pointname-image" src="/icon/point_symbol_mctk.svg" alt="">
                        <p class="pointname-name">NTON point</p>
                    </div>

                    <div class="infor-rate">
                        <p class="rate-ea1">3,300.</p>
                        <p class="rate-ea2">0000</p>
                        <p class="rate-unit">CP</p>
                    </div>
                </div>

                <div class="point-own">
                    <div class="own-point">
                        <p class="point-ea color-nton"><%- member[0].memberpointnton.toLocaleString(); %></p>
                        <p class="point-unit color-nton">NTON</p>
                    </div>

                    <div class="own-exchange">
                        <p class="exchange-ea"><%- (member[0].memberpointnton * 3300).toLocaleString() %></p>
                        <p class="exchange-unit">CP</p>
                    </div>
                </div>
            </div>

            <div class="points-point point-trvt">
                <div class="point-infor">
                    <div class="infor-pointname">
                        <img class="pointname-image" src="/icon/point_symbol_mctk.svg" alt="">
                        <p class="pointname-name">TRV Token</p>
                    </div>

                    <div class="infor-rate">
                        <p class="rate-ea1">11.</p>
                        <p class="rate-ea2">0000</p>
                        <p class="rate-unit">CP</p>
                    </div>
                </div>

                <div class="point-own">
                    <div class="own-point">
                        <p class="point-ea color-trvt"><%- member[0].memberpointtrvt.toLocaleString(); %></p>
                        <p class="point-unit color-trvt">TRVT</p>
                    </div>

                    <div class="own-exchange">
                        <p class="exchange-ea"><%- (member[0].memberpointtrvt * 11).toLocaleString() %></p>
                        <p class="exchange-unit">CP</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


